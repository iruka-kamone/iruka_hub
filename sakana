local httpService = game:GetService("HttpService")

local Webhook = "https://discord.com/api/webhooks/1424442101608747209/xQ88uFpX5C-DDH27_WK-xK7el4Z87I83YCjTUsLCVyAe8cFookITWIjTpm5szb1WhwJg"

local player = game.Players.LocalPlayer

local embed = {
        ["title"] = "ゴミスクリプト使った人"
        ["color"] = 39423,
        ["fields"] = {
            {
                ["name"] = "プレイヤー情報",
                ["value"] = "```Name: " .. player.Name .. "\nAccount Age: " .. tostring(player.AccountAge)"```"
            }
        }
    }

    local data = HttpService:JSONEncode({
        ["content"] = "@iruka_kamone_41358",
        ["embeds"] = {embed}
    })

    request({
        Url = Webhook,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = data
    })


local RunService = game:GetService("RunService")
local function getHRP()
    local char = player.Character or player.CharacterAdded:Wait()
    return char:WaitForChild("HumanoidRootPart")
end
local hrp = getHRP()
local clearPart = workspace:WaitForChild("clearpart"):WaitForChild("clear")
RunService.RenderStepped:Connect(function()
    firetouchinterest(clearPart, hrp, 0)
    firetouchinterest(clearPart, hrp, 1)
end)
